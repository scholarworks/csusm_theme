      <div class="field-items">
        <div class="field-item even">
          <ul class="nav nav-list">
              <% if user_signed_in? %>
                <li>
                  <%= link_to sufia.notifications_path do %>
                    <span class="icon-wrapper"><i class="fa fa-envelope"></i></span>
                    <%= t("sufia.toolbar.notifications") %>
                    <%= render partial: 'users/notify_number' %>
                  <% end %>
                </li>
                <li>
                  <%= link_to sufia.profile_path(current_user) do %>
                    <span class="icon-wrapper"><i class="fa fa-user"></i></span>
                    <%= current_user.name %>
                  <% end %>
                </li>
                <li>
                  <%= link_to main_app.destroy_user_session_path do %>
