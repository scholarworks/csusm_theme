              <% else %>
                <li>
                  <%= link_to main_app.new_user_session_path do %>
                    <span class="icon-wrapper"><i class="glyphicon glyphicon-log-in" aria-hidden="true"></i></span> <%= t("sufia.toolbar.profile.login") %>
